{
  "name": "petak",
  "version": "1.0.0",
  "description": "Image comparator service",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "PETAK_ENV=TEST ./node_modules/.bin/nyc --reporter=text-summary ./node_modules/.bin/mocha",
    "cover": "PETAK_ENV=TEST ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "coverage": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "author": "Luthfi Kurnia Putra",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "mocha": "5.1.0",
    "mongoose": "^5.2.5",
    "multer": "^1.3.1",
    "mz": "^2.7.0",
    "npm": "^5.10.0",
    "nyc": "^11.9.0",
    "resemblejs": "^2.10.3",
    "sinon": "^4.5.0",
    "sinon-mongoose": "^2.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
